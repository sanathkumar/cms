<%= error_messages_for(@subject) %>

<table summary="Subject form fields">
      <tr>
        <th><%= f.label(:name)%></th>
        <td><%= f.text_field(:name) %></td>
      </tr>
      <tr>
        <th><%= f.label(:postion)%></th>
        <td><%= f.select(:postion, 1..@subject_count) %></td>
      </tr>
      <tr>
    <th><%= f.label(:visible)%></th>
    <td><%= f.check_box(:visible) %></td>
  </tr>
      <tr>
        <th><%= f.label(:created_at, "Created")%></th>
        <td><%= f.datetime_select(:created_at, :order => [:month,:day,:year]) %></td>
      </tr>
    </table>